{
  "name": "mini-mern-app",
  "version": "1.0.0",
  "description": "Mini MERN App",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client
      && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karabayse/mini-mern-app.git"
  },
  "author": "Kara Bayse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karabayse/mini-mern-app/issues"
  },
  "homepage": "https://github.com/karabayse/mini-mern-app#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.4",
    "mongoose": "^4.10.8"
  },
  "devDependencies": {
    "nodemon": "^1.18.6"
  }
}
